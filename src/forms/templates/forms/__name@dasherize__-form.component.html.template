<app-section title="<%= formJsonObj.title %>">
    <form class="form" [formGroup]="this.<%= camelize(name) %>Form">
        <button type="submit" [disabled]="!this.<%= camelize(name) %>Form.status" (click)="onSubmit()" class="btn btn-primary">
            Submit
        </button>
    </form>
</app-section>